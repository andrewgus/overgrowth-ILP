---
type Props = {
	imgSrc: string
	altText?: string
	describedBy?: string
}
const { imgSrc, altText = '', describedBy } = Astro.props
---

<img
	aria-describedby={describedBy}
	src=`https://fscjcel.blob.core.windows.net/ilp-assets/${Astro.url.toString().split('/').at(-1)}:${imgSrc}`
	alt={altText}
/>
